{"components/common/social-links":"{{#if social_media}}\n    <ul class=\"socialLinks socialLinks--alt\">\n        {{#each social_media}}\n            {{#if display_name '===' 'twitter'}}\n                <li class=\"socialLinks-item\">\n                    <a class=\"icon icon--x\"\n                    href=\"{{url}}\"\n                    target=\"_blank\"\n                    rel=\"noopener\"\n                    title=\"X\"\n                    >\n                        <span class=\"aria-description--hidden\">X</span>\n                        <svg>\n                            <use href=\"#icon-x\"/>\n                        </svg>\n                    </a>\n                </li>\n            {{else}}\n                <li class=\"socialLinks-item\">\n                    <a class=\"icon icon--{{display_name}}\"\n                    href=\"{{url}}\"\n                    target=\"_blank\"\n                    rel=\"noopener\"\n                    title=\"{{{capitalize display_name}}}\"\n                    >\n                        <span class=\"aria-description--hidden\">{{{capitalize display_name}}}</span>\n                        <svg>\n                            <use href=\"#icon-{{display_name}}\"/>\n                        </svg>\n                    </a>\n                </li>\n            {{/if}}\n        {{/each}}\n    </ul>\n{{/if}}\n","components/custom/page/footer-interim":"<footer class=\"footer\" role=\"contentinfo\">\r\n    <h2 class=\"footer-title-sr-only\">{{lang 'footer.title'}}</h2>\r\n    <div class=\"container\">\r\n\t\t<a href=\"{{urls.home}}\">\r\n\t\t\t<img src=\"{{cdn 'img/OPTADERM_Logo_White.webp'}}\" width=\"300\" height=\"46\" alt=\"{{settings.store_name}} Logo\"/>\r\n\t\t</a>\r\n\r\n\t\t<article class=\"footer-social-links\" data-section-type=\"footer-webPages\">\r\n\t\t\t\t{{> components/common/social-links}}\r\n\t\t</article>\r\n\r\n        {{#if theme_settings.show_copyright_footer}}\r\n            <div class=\"footer-copyright\">\r\n                <p class=\"powered-by\">&copy; {{moment format=\"YYYY\"}} {{settings.store_name}} Products. All rights reserved.</p>\r\n            </div>\r\n        {{/if}}\r\n    </div>\r\n</footer>\r\n"}
