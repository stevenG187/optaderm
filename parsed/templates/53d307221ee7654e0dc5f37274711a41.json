{"components/common/social-links":"{{#if social_media}}\n    <ul class=\"socialLinks socialLinks--alt\">\n        {{#each social_media}}\n            {{#if display_name '===' 'twitter'}}\n                <li class=\"socialLinks-item\">\n                    <a class=\"icon icon--x\"\n                    href=\"{{url}}\"\n                    target=\"_blank\"\n                    rel=\"noopener\"\n                    title=\"X\"\n                    >\n                        <span class=\"aria-description--hidden\">X</span>\n                        <svg>\n                            <use href=\"#icon-x\"/>\n                        </svg>\n                    </a>\n                </li>\n            {{else}}\n                <li class=\"socialLinks-item\">\n                    <a class=\"icon icon--{{display_name}}\"\n                    href=\"{{url}}\"\n                    target=\"_blank\"\n                    rel=\"noopener\"\n                    title=\"{{{capitalize display_name}}}\"\n                    >\n                        <span class=\"aria-description--hidden\">{{{capitalize display_name}}}</span>\n                        <svg>\n                            <use href=\"#icon-{{display_name}}\"/>\n                        </svg>\n                    </a>\n                </li>\n            {{/if}}\n        {{/each}}\n    </ul>\n{{/if}}\n","components/common/subscription-form":"<!-- <h3 class=\"footer-info-heading\">{{lang 'newsletter.subscribe'}}</h3> \r\n<p>{{lang 'newsletter.subscribe_intro'}}</p> -->\r\n<h3>Keep up with the latest Optaderm products, skincare tips, exclusive offers, and more.</h3>\r\n\r\n<form class=\"form\" action=\"{{urls.subscribe.action}}\" method=\"post\">\r\n    <fieldset class=\"form-fieldset\">\r\n        <input type=\"hidden\" name=\"action\" value=\"subscribe\">\r\n        <input type=\"hidden\" name=\"nl_first_name\" value=\"bc\">\r\n        <input type=\"hidden\" name=\"check\" value=\"1\">\r\n        <div class=\"form-field\">\r\n            <label class=\"form-label is-srOnly\" for=\"nl_email\">{{lang 'common.email_address'}}</label>\r\n            <div class=\"form-prefixPostfix wrap\">\r\n                <input class=\"form-input\"\r\n                       id=\"nl_email\"\r\n                       name=\"nl_email\"\r\n                       type=\"email\"\r\n                       value=\"{{customer.email}}\"\r\n                       placeholder=\"Email Address\"\r\n                       aria-describedby=\"alertBox-message-text\"\r\n                       aria-required=\"true\"\r\n                       autocomplete=\"email\"\r\n                       required\r\n                >\r\n                <input class=\"button button--primary form-prefixPostfix-button--postfix\"\r\n                       type=\"submit\"\r\n                       value=\"Sign me up\"\r\n                >\r\n            </div>\r\n            {{#if settings.show_newsletter_summary }}\r\n                <div class=\"footer-newsletter-summary\">{{settings.newsletter_summary}}</div>\r\n            {{/if}}\r\n        </div>\r\n        \r\n        <!-- Terms input section -->\r\n        <div class=\"form-field\">\r\n            <label for=\"terms-checkbox\" class=\"form-label\">\r\n                <input type=\"checkbox\" id=\"terms-checkbox\" name=\"terms\" required>\r\n                I have read and agree to the <a href=\"/terms-of-service\" target=\"_blank\">terms of service</a> and <a href=\"/privacy-policy\" target=\"_blank\">privacy policy</a>, and agree to receive emails.\r\n            </label>\r\n        </div>\r\n    </fieldset>\r\n</form>\n","components/custom/custom-footer":"<hr class=\"footer-line\">\r\n\r\n<div class=\"footer-cta page\">  \r\n    <div class=\"flex-row align-center\">\r\n        <div class=\"w-30 text-center\">\r\n            <h2>FROM US TO YOU</h2>\r\n        </div>\r\n        <div class=\"w-70\">\r\n            {{> components/common/subscription-form}}\r\n        </div>\r\n    </div>\r\n</div>\r\n\r\n<footer class=\"footer page\" role=\"contentinfo\">\r\n    <div class=\"container flex-row\">\r\n        <div class=\"footer-item about-menu w-20\">\r\n            <h3 class=\"footer-info-heading\">About</h3>\r\n            <ul class=\"footer-info-list\">\r\n                <li><a href=\"/about\">About Us</a></li>\r\n                <li><a href=\"/shop\">Shop</a></li>\r\n                <li><a href=\"/shop/face/\">Face</a></li>\r\n                <li><a href=\"/shop/body/\">Body</a></li>\r\n                <li><a href=\"/skin-concerns\">Skin Concerns</a></li>\r\n                <li><a href=\"/clinic\">Clinic</a></li>\r\n                <!-- <li><a href=\"#\">Affiliates</a></li> -->\r\n            </ul>\r\n        </div>\r\n        <div class=\"footer-item support-menu w-20\">\r\n            <h3 class=\"footer-info-heading\">Support</h3>\r\n            <ul class=\"footer-info-list\">\r\n                <li><a href=\"/faq\">FAQs</a></li>\r\n                <li><a href=\"/privacy-policy\">Legal</a></li>\r\n                <li><a href=\"/contact-us\">Contact Us</a></li>\r\n            </ul>\r\n        </div>\r\n        <div class=\"footer-item social-media w-20\">\r\n            {{> components/common/social-links}}\r\n        </div>\r\n        <div class=\"footer-item info w-40\">\r\n            <div class=\"logo\">\r\n                <img src=\"{{cdn 'img/OPTADERM_Logo_White.webp'}}\" width=\"300\" height=\"46\" alt=\"{{settings.store_name}} Logo\"/>\r\n                <p>Science for the skin youâ€™ll love for life.</p>\r\n            </div>\r\n            <div class=\"powered-links\">\r\n                <ul class=\"links flex-row\">\r\n                    <li><a href=\"/terms-and-conditions/\">Privacy Policy</a></li>\r\n                    <li><a href=\"/terms-of-service/\">Terms of Service</a></li>\r\n                </ul>\r\n                <p class=\"powered-by\">&copy; {{moment format=\"YYYY\"}} {{settings.store_name}} Products. All rights reserved.</p>\r\n            </div>\r\n        </div>\r\n    </div>\r\n</footer>\r\n"}
